    @RenderSection("profilePage", false)
    <script src="~/Scripts/Custom/GoogleAnalytics.js"></script>
    <script>
	$(document).ready(function(){
		var MsgDn = false;
		var ReqDn = false;
		var FrnDn =false
		var LogDn = false;
		$(".fa-envelope").click (function(){
			$(".dropdown-menu").slideUp();
			ReqDn = false;
			FrnDn = false;
			LogDn = false;
			if (!MsgDn){
				$("#message-list").slideDown();
				MsgDn = true;
			}
			else MsgDn=false;
			});
		$(".fa-user-plus").click (function(){
			$(".dropdown-menu").slideUp();
			MsgDn = false;
			FrnDn = false;
			LogDn = false;
			if (!ReqDn){
				$("#friend-request-list").slideDown();
				ReqDn = true;
			}
			else ReqDn=false;
			});
		$(".fa-group").click (function(){
			$(".dropdown-menu").slideUp();
			MsgDn = false;
			ReqDn = false;
			LogDn = false;
			if (!FrnDn){
				$("#navbar-friend-list").slideDown();
				FrnDn = true;
			}
			else FrnDn=false;
		});
		$("#login-icon").click (function(){
			$(".dropdown-menu").slideUp();
			MsgDn = false;
			ReqDn = false;
			FrnDn = false;
			if (!LogDn){
				$(".login-dropdown").slideDown();
				LogDn = true;
			}
			else LogDn=false;
		});
	});